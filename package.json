{
  "name": "uhhhh",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "start": "turbo start",
    "build": "turbo build",
    "dev": "turbo dev",
    "api:dev": "turbo dev --filter=api... --no-cache",
    "api:build": "turbo build --filter=api",
    "web:dev": "turbo dev --filter=web... --no-cache",
    "web:build": "turbo build --filter=web",
    "clean": "turbo clean",
    "typecheck": "turbo typecheck"
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "turbo": "^2.2.3"
  },
  "engines": {
    "node": ">=20.6.0"
  },
  "packageManager": "yarn@4.5.3"
}
