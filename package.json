{
  "name": "uhhhh",
  "private": true,
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ]
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --no-cache --continue",
    "api:dev": "turbo run dev --filter=api... --no-cache",
    "api:build": "turbo run build --filter=api",
    "web:dev": "turbo run dev --filter=web... --no-cache",
    "web:build": "turbo run build --filter=web"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^1.13.3"
  },
  "packageManager": "yarn@1.22.11"
}
